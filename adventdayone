#Advent Day One
def parser(number):
    pos_neg = number[0]
    value = number[1:]
    value = value.replace("/n","")
    value = value.replace(" ","")
    value = int(value)
    if (pos_neg == "-"):
        return value* -1
    else:
        return value
    

freq = 0

with open("./Adventofcode/dayone.txt", "r") as file:
    for line in file:
        value = parser(line)
        freq += value
    print (freq)
